    "deploy": "npm run build:prod && firebase deploy",
    "bundle-report": "webpack-bundle-analyzer dist/ng-pokedex/stats.json",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
