  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "ng build --prod && lite-server --baseDir dist/ng-pokedex",
    "build": "ng build",
    "build:prod": "npm run lint && ng build --prod --stats-json",
    "test": "ng test",
    "lint": "sass-lint && ng lint",
