    "@angular/forms": "~9.1.2",
    "@angular/platform-browser": "~9.1.2",
    "@angular/platform-browser-dynamic": "~9.1.2",
    "@angular/pwa": "^0.901.1",
    "@angular/router": "~9.1.2",
    "@angular/service-worker": "~9.1.2",
    "@ngx-lite/modal": "^0.5.5",
    "@ngx-lite/nav-drawer": "^0.4.5",
    "@ngx-lite/util": "0.0.0",
    "blueprint-css": "3.1.1",
    "normalize.css": "^8.0.1",
